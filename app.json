{
  "expo": {
    "name": "FomoV2",
    "slug": "FomoV2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fomov2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBkRcDClCTq7KlFuhiHQ8SH4x6cxKUpBR4"
      }
    },
    "plugins": [
      "expo-font"
    ]
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true,
    "config": {
      "googleMaps": {
        "apiKey": "AIzaSyBkRcDClCTq7KlFuhiHQ8SH4x6cxKUpBR4"
      }
    }
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash-icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    ],
    "react-native-maps"
  ],
  "experiments": {
    "typedRoutes": true
  }
}
